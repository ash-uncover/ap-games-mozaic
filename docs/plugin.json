{
  "name": "mozaic",
  "url": "https://ash-uncover.github.io/ap-games-mozaic",
  "dependencies": [
    "https://ash-uncover.github.io/ap-games-mozaic-themes/plugin.json"
  ],
  "provides": {
    "ap-games/game": {
      "name": "mozaic",
      "attributes": {
        "name": "Mozaic",
        "description": "A mozaic game",
        "thumbnail": "/images/thumbnail.jpg"
      },
      "elements": {
        "main": {
          "url": "#",
          "type": "iframe"
        }
      }
    },
    "mozaic/theme": {
      "name": "default",
      "attributes": {
        "name": "Default",
        "description": "Default theme",
        "thumbnail": "/theme/thumbnail.jpg",
        "background": "/theme/background.jpg",
        "images": [
          "/theme/image_0.jpg",
          "/theme/image_1.jpg",
          "/theme/image_2.jpg",
          "/theme/image_3.jpg",
          "/theme/image_4.jpg",
          "/theme/image_5.jpg",
          "/theme/image_6.jpg",
          "/theme/image_7.jpg",
          "/theme/image_8.jpg",
          "/theme/image_9.jpg"
        ]
      }
    }
  },
  "defines": {
    "theme": {
      "attributes": {
        "name": "string",
        "description": "string",
        "thumbnail?": "url",
        "background?": "url",
        "images": "url[]",
        "music?": "url[]"
      }
    }
  }
}