{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "ES2020",
      "ESNext",
      "ESNext.Intl"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "dist",
    "paths": {
      "components": [
        "src/components"
      ],
      "components/*": [
        "src/components/*"
      ],
      "lib": [
        "src/lib"
      ],
      "lib/*": [
        "src/lib/*"
      ],
      "route": [
        "src/route"
      ],
      "routes/*": [
        "src/routes/*"
      ],
      "services": [
        "src/services"
      ],
      "services/*": [
        "src/services/*"
      ],
      "store": [
        "src/store"
      ],
      "store/*": [
        "src/store/*"
      ],
      "*": [
        "src/*"
      ],
    },
    "sourceMap": false,
    "strict": true,
    "target": "es6",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "test",
    ".vscode"
  ]
}