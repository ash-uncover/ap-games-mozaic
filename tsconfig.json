{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "paths": {
      "components": [
        "src/components"
      ],
      "components/*": [
        "src/components/*"
      ],
      "lib": [
        "src/lib"
      ],
      "lib/*": [
        "src/lib/*"
      ],
      "route": [
        "src/route"
      ],
      "routes/*": [
        "src/routes/*"
      ],
      "services": [
        "src/services"
      ],
      "services/*": [
        "src/services/*"
      ],
      "store": [
        "src/store"
      ],
      "store/*": [
        "src/store/*"
      ],
      "*": [
        "src/*"
      ],
    },
    "target": "es6",
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "test",
    ".vscode"
  ]
}